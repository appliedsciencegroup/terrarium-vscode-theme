{
  "name": "terrarium-theme",
  "displayName": "Terrarium Theme",
  "description": "A minimal theme with light and dark variants that focuses on readability, clear error/alert highlighting, and drawing attention to important elements in code and terminal output.",
  "version": "0.3.0",
  "publisher": "appliedsciencegroup",
  "author": {
    "name": "Applied Science Group"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/appliedsciencegroup/terrarium-vscode-theme.git"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Terrarium (Dark)",
        "uiTheme": "vs-dark",
        "path": "./themes/terrarium-dark.json"
      },
      {
        "label": "Terrarium (Light)",
        "uiTheme": "vs",
        "path": "./themes/terrarium-light.json"
      }
    ]
  },
  "galleryBanner": {
    "color": "#242424",
    "theme": "dark"
  },
  "keywords": [
    "theme",
    "dark theme",
    "light theme",
    "terrarium",
    "minimal",
    "readable",
    "accessible",
    "greyscale",
    "gold",
    "green",
    "distraction-free"
  ],
  "icon": "icon.png",
  "license": "MIT",
  "scripts": {
    "package": "vsce package"
  }
}
