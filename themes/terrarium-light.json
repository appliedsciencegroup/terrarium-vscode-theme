	"tokenColors": [{
			"settings": {
				"foreground": "#4F5252"
			}
		},
		{
			"scope": [
				"support.function",
				"keyword.operator.accessor",
				"meta.group.braces.round.function.arguments",
				"meta.template.expression",
				"markup.fenced_code meta.embedded.block"
			],
			"settings": {
				"foreground": "#BBA85F"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["strong", "markup.bold.markdown"],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#BBA85F"
			}
		},
		{
			"scope": ["markup.heading.markdown"],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown H1",
			"scope": "markup.heading.1.markdown",
			"settings": {
				"foreground": "#BBA85F",
				"fontStyle": "bold",
				"textMateRules": {
					"textDecorations": "text-shadow: 0 0 4px #BBA85F40;"
				}
			}
		},
		{
			"name": "Markdown H2",
			"scope": "markup.heading.2.markdown",
			"settings": {
				"foreground": "#A5945A",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown H3",
			"scope": "markup.heading.3.markdown",
			"settings": {
				"foreground": "#978854",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown H4",
			"scope": "markup.heading.4.markdown",
			"settings": {
				"foreground": "#887C4E",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown H5",
			"scope": "markup.heading.5.markdown",
			"settings": {
				"foreground": "#797047",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markdown H6",
			"scope": "markup.heading.6.markdown",
			"settings": {
				"foreground": "#6B6441",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"markup.italic.markdown"
			],
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "meta.link.inline.markdown",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#3F5A3B"
			}
		},
		{
			"scope": [
				"string",
				"markup.fenced_code",
				"markup.inline"
			],
			"settings": {
				"foreground": "#3F5A3B"
			}
		},
		{
			"scope": [
				"comment",
				"string.quoted.docstring.multi"
			],
			"settings": {
				"foreground": "#AAAAAA"
			}
		},
		{
			"scope": [
				"constant.language",
				"variable.language.this",
				"variable.other.object",
				"variable.other.class",
				"variable.other.constant",
				"meta.property-name",
				"support",
				"string.other.link.title.markdown"
			],
			"settings": {
				"foreground": "#3F5A3B"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.other.placeholder",
				"constant.character.format.placeholder",
				"meta.property-value",
				"keyword.other.unit",
				"keyword.other.template",
				"entity.name.tag.yaml",
				"entity.other.attribute-name",
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#4F5252"
			}
		},
		{
			"scope": [
				"keyword",
				"storage.modifier",
				"storage.type",
				"storage.control.clojure",
				"entity.name.function.clojure",
				"support.function.node",
				"punctuation.separator.key-value",
				"punctuation.definition.template-expression"
			],
			"settings": {
				"foreground": "#BBA85F"
			}
		},
		{
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#E0A030"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"meta.function-call",
				"meta.instance.constructor",
				"entity.other.attribute-name",
				"entity.name.function",
				"constant.keyword.clojure"
			],
			"settings": {
				"foreground": "#BBA85F"
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"string.quoted",
				"string.regexp",
				"string.interpolated",
				"string.template",
				"string.unquoted.plain.out.yaml",
				"keyword.other.template"
			],
			"settings": {
				"foreground": "#3F5A3B"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#3F5A3B"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#E0A030"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#D83933"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#BBA85F"
			}
		},
		{
			"scope": [
				"punctuation.definition.arguments",
				"punctuation.definition.dict",
				"punctuation.separator",
				"meta.function-call.arguments"
			],
			"settings": {
				"foreground": "#4F5252"
			}
		},
		{
			"name": "[Custom] Markdown paragraph text",
			"scope": "text.html.markdown meta.paragraph.markdown",
			"settings": {
				"foreground": "#4F5252"
			}
		},
		{
			"name": "[Custom] Markdown links",
			"scope": "markup.underline.link",
			"settings": {
				"foreground": "#3F5A3B"
			}
		},
		{
			"name": "[Custom] Markdown list",
			"scope": [
				"beginning.punctuation.definition.list.markdown"
			],
			"settings": {
				"foreground": "#BBA85F"
			}
		},
		{
			"name": "[Custom] Markdown punctuation definition",
			"scope": "punctuation.definition.metadata.markdown",
			"settings": {
				"foreground": "#3F5A3B"
			}
		},
		{
			"name": "[Custom] Markdown punctuation definition brackets",
			"scope": [
				"punctuation.definition.string.begin.markdown",
				"punctuation.definition.string.end.markdown"
			],
			"settings": {
				"foreground": "#3F5A3B"
			}
		}
	],
	"semanticHighlighting": true
}
